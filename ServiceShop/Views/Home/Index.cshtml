@{
    ViewBag.Title = "Home Page";
}
<style>
    #myCarousel {
        height: 360px;
    }

    #instructionsbox {
        font-size: 2em;
    }
</style>
<img src="~/Content/Images/ComputerDoctor.jpg" width="1920" height="625" alt="PC Doctor" style="width:1140px;height:250px;">
<div>
    <center>
        @*<h1>IseSystems Computer Services LLC</h1>*@
        <img src="~/Content/Images/IseSystemsComputerServicesLogo3.gif" width="1061" height="97" alt="PC Doctor" style="width:1140px;height:97px;">
    </center>
</div>
<h2>Our Process</h2>

<div id="instructionsbox">
    <ol>
        <li>Register for an account!</li>
        <li>Submit a Work Order</li>
        <li>Communications With Our Friendly Staff</li>
        <li>Track Work Progress Status</li>
        <li>Rate Our Performance</li>
    </ol>
</div>

<h2>Our Sponsors</h2>
<div id="myCarousel" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
        <div class="item active">
            <img src="/Content/Images/Image1_1920x360.jpg" alt="" style="height:360px;">
        </div>

        <div class="item">
            <img src="/Content/Images/Image2_1920x360.jpg" alt="" style="height:360px;">
        </div>

        <div class="item">
            <img src="/Content/Images/Image3_1920x360.jpg" alt="" style="height:360px;">
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
<div>
    <div id="chatbox"></div>
    <div id="emailbox"></div>
    <div id="phonebox"></div>
</div>